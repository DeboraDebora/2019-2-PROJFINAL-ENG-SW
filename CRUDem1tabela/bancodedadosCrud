CREATE SCHEMA TrampoFacil;

use TrampoFacil;  

--criacao das tabelas
CREATE TABLE Usuario
{
		nome int primary key,
		tipoServico varchar(30),
		cpf varchar(11),
		email varchar (20),
		telefone integer(11);
		
		
}


CREATE TABLE Contrato
{
	idContrato int primary key;
	valor float (10,2);
	prazo varchar(10);
	Servico varchar(50);
	usuario varchar(50) foreign key; 
	usuario varchar(50) foreign key;

}
